          (function ($) {
            $(document).on("click", "a.open-popup-product", function(e) {
              e.preventDefault();
              var id = $(this).attr('data-id');
              //console.log('id: ' + id);
              var noidungpopup = $('#product' + id).html();
              //console.log('noidungpopup: ' + noidungpopup);
              $("#myModal .iframe-load").html(noidungpopup);
              $("#myModal").modal("show");
              return false;

